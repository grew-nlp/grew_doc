[GRAPH] {scale=200; fontname="Arial"; background=transparent; }
[WORDS] {
  N_0 { word="_" }
  N_1 { word="Je"; subword="#il#PRO"}
  N_2 { word="suis"; subword="#suivre#VERB"}
  N_3 { word="une"; subword="#un#DET"}
  N_4 { word="licorne"; subword="#licorne#NOUN"}
}
[EDGES] {
  N_2 -> N_1 { label="suj"; bottom }
  N_0 -> N_2 { label="root"; bottom }
  N_4 -> N_3 { label="det"; bottom }
  N_2 -> N_4 { label="obj"; bottom }
}

